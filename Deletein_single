import java.util.*;
class Node{
    int data;
    Node next=null;
    Node(int data){
        this.data=data;
        this.next=null;
    }
}
public class Main {
    public static void main(String[] args) {
     Scanner obj=new Scanner(System.in);
    String[]a=obj.nextLine().split(" ");
    Node head=null;
    Node temp=null;
    int pos=Integer.parseInt(a[0]);
   for(int i=1;i<a.length;i++){
Node newnode=new Node(Integer.parseInt(a[i]));
    if(head==null){
        head=newnode;
        temp=head;
    }else{
      temp.next=newnode;
      temp=newnode;
    }
   }
if(pos==0){
    head=head.next;
}else{
    Node prev=null;
    temp=head;
for(int i=0;i<pos;i++){
    prev=temp;
    temp=temp.next;
}
prev.next=temp.next;
}
temp=head;
while(temp!=null){
    System.out.print(temp.data);
    if(temp.next!=null){
        System.out.print(" -> ");
    }
    temp=temp.next;
}



    }
}
